{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "eleventy && tailwindcss -i src/styles.css -o public/styles.css --minify",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i src/styles.css -o public/styles.css --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4"
  }
}
